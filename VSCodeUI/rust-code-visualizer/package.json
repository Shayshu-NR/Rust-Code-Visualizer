{
  "name": "rust-code-visualizer",
  "displayName": "Rust-Code-Visualizer",
  "description": "VScode extension that helps developers visualize Rust code and view performance metrics",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:rust-code-visualizer.helloWorld",
    "onCommand:rust-code-visualizer.Graph",
    "onView:rust-sidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rust-sidebar-view",
          "title": "Rust Graph Viewer",
          "icon": "media/RustLogo.svg"
        }
      ]
    },
    "views": {
      "rust-sidebar-view": [
        {
          "type": "webview",
          "id": "rust-sidebar",
          "name": "Rust Graph",
          "icon": "media/RustLogo.svg",
          "contextualTitle": "RustGraph"
        }
      ]
    },
    "commands": [
      {
        "command": "rust-code-visualizer.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "rust-code-visualizer.Graph",
        "title": "Graph Rust Code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config ./webpack.config.js",
    "watch": "webpack --watch --config ./webpack.config.js",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "build": "react-scripts build",
    "start": "react-scripts start"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "bootstrap-icons": "^1.9.1",
    "chart.js": "^3.9.1",
    "classnames": "^2.3.2",
    "cytoscape": "^3.23.0",
    "faker": "^6.6.6",
    "fuse.js": "^6.6.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-cytoscapejs": "^2.0.0",
    "react-dom": "^18.2.0",
    "react-loading": "^2.0.3",
    "react-paginate": "^8.1.4",
    "react-scripts": "5.0.1",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.2.1",
    "web-vitals": "^2.1.4",
    "wsl-path": "^4.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
